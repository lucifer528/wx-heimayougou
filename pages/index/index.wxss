navigator {
  width: 100%;
  height: 100%;
}
.index_swiper swiper {
  height: calc(750rpx * 340 / 750);
}
.index_swiper swiper swiper-item image {
  width: 100%;
  height: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor {
  padding: 10rpx;
}
.index_floor .product {
  overflow: hidden;
}
.index_floor .product navigator {
  float: left;
  width: 33.33%;
  height: calc(750rpx * 0.33 * 386 / 232 / 2);
}
.index_floor .product navigator image {
  width: 100%;
  height: 100%;
}
.index_floor .product navigator:nth-last-child(-n+4) {
  padding-left: 10rpx;
}
.index_floor .product navigator:nth-child(2),
.index_floor .product navigator:nth-child(3) {
  padding-bottom: 10rpx;
}
.index_floor .product navigator:first-child {
  height: calc(750rpx * 0.33 * 386 / 232);
}
